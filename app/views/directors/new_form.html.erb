<h1>Add A New Director</h1>

<form action="/create_director">
  <div>
    <label>Name:</label>
    <input type="text" name="the_name">
  </div>
  <div>
    <label>Bio:</label>
    <input type="text" name="the_bio">
  </div>
  <div>
    <label>Date of Birth:</label>
    <input type="text" name="the_dob">
  </div>
  <div>
    <label>Image URL:</label>
    <input type="text" name="the_url">
  </div>
  <div>
    <button>Create Director</button>
  </div>

</form>
